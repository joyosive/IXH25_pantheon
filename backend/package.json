{
  "name": "xrpl-zkp-circom",
  "version": "1.0.0",
  "description": "Zero-knowledge proof implementation using Circom for XRPL EVM sidechain",
  "main": "index.js",
  "scripts": {
    "setup": "npm run install-circom && npm run compile-age && npm run compile-docs",
    "install-circom": "npm install -g circom",
    "compile-age": "circom circuits/age_verify.circom --r1cs --wasm --sym -o build/",
    "compile-docs": "circom circuits/document_verify.circom --r1cs --wasm --sym -o build/",
    "compile-circuits": "npm run compile-age && npm run compile-docs",
    "generate-witness": "node scripts/generate_witness.js",
    "generate-doc-witness": "node scripts/generate_doc_witness.js",
    "generate-proof": "node scripts/generate_proof.js",
    "generate-doc-proof": "node scripts/generate_doc_proof.js",
    "verify-proof": "node scripts/verify_proof.js",
    "verify-doc-proof": "node scripts/verify_doc_proof.js",
    "dev": "http-server -p 3001",
    "deploy": "node scripts/deploy.js",
    "deploy-grant": "node scripts/deploy_grant.js",
    "deploy-live": "node scripts/deploy_grant_live.js",
    "deploy-grant-program": "node scripts/deploy_grant_program.js",
    "deploy-grant-simple": "node scripts/deploy_grant_simple.js",
    "test-zk-proof": "node scripts/test_zk_proof.js",
    "test-zk-integration": "node scripts/test_zk_integration.js",
    "test-real-verifier": "node scripts/use_real_verifier.js",
    "deploy-web3": "node scripts/deploy_grant_web3.js",
    "test-full-workflow": "node scripts/test_full_workflow.js",
    "deploy-simple-grant": "node scripts/deploy_simple_grant.js",
    "deploy-actual-grant": "node scripts/deploy_actual_grant.js",
    "deploy-minimal-grant": "node scripts/deploy_minimal_grant.js",
    "deploy-working-grant": "node scripts/deploy_working_grant.js",
    "deploy-direct-grant": "node scripts/deploy_direct_grant.js",
    "test-demo-workflow": "node scripts/test_demo_workflow.js",
    "deployment-plan": "node scripts/deploy_simple.js"
  },
  "keywords": [
    "zkp",
    "circom",
    "xrpl",
    "sidechain",
    "zero-knowledge"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "circomlib": "^2.0.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.8.1",
    "snarkjs": "^0.7.3",
    "xrpl": "^3.0.0"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  }
}
