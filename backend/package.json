{
  "name": "xrpl-zkp-circom",
  "version": "1.0.0",
  "description": "Zero-knowledge proof implementation using Circom for XRPL EVM sidechain",
  "main": "index.js",
  "scripts": {
    "setup": "npm run install-circom && npm run compile-circuits",
    "install-circom": "npm install -g circom",
    "compile-circuits": "circom circuits/age_verify.circom --r1cs --wasm --sym -o build/",
    "generate-witness": "node scripts/generate_witness.js",
    "generate-proof": "node scripts/generate_proof.js",
    "verify-proof": "node scripts/verify_proof.js",
    "dev": "http-server -p 3001",
    "deploy": "node scripts/deploy.js"
  },
  "keywords": [
    "zkp",
    "circom",
    "xrpl",
    "sidechain",
    "zero-knowledge"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "circomlib": "^2.0.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.8.1",
    "snarkjs": "^0.7.3",
    "xrpl": "^3.0.0"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  }
}
